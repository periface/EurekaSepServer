@model Eureka.Spe.Scholarships.Dto.ScholarshipDto

@{
    ViewBag.ActiveMenu = PageNames.Scholarships; //The menu item will be active for this page.
}
<div class="row clearfix">
    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
        <div class="card">
            <div class="header">
                <h2>
                    @Model.Title
                </h2>
            </div>

            <div class="body table-responsive">
                <ul class="nav nav-tabs" role="tablist">
                    <li role="presentation" class="active"><a href="#home" aria-controls="home" role="tab" data-toggle="tab">Contenido</a></li>
                    <li role="presentation"><a href="#profile" aria-controls="profile" role="tab" data-toggle="tab">Notificaciones</a></li>
                    <li role="presentation"><a href="#messages" aria-controls="messages" role="tab" data-toggle="tab">Secciones</a></li> 
                </ul>
                <div class="tab-content">
                    <div role="tabpanel" class="tab-pane active" id="home">
                        

                    </div>
                    <div role="tabpanel" class="tab-pane" id="profile">
                        <button type="button" class="btn btn-primary">Agendar</button>
                    </div>
                    <div role="tabpanel" class="tab-pane" id="messages">
                        <button type="button" class="btn btn-primary">Agregar</button>
                    </div>
                    <div role="tabpanel" class="tab-pane" id="settings">...</div>
                </div>
            </div>
        </div>
    </div>
</div>
@section Scripts{
    <script src="~/lib/jquery-validation/dist/jquery.validate.min.js"></script>
    <script src="~/lib/jquery-validation/dist/additional-methods.min.js"></script>
    <script src="~/js/eModal/eModal.min.js"></script>
    <script src="~/js/bootstraptable/bootstrap-table.min.js"></script>
    <script src="~/js/bootstraptable/bootstrap-table-es-MX.min.js"></script>
    <script src="~/Views/Scholarships/Manage.js"></script>
}
