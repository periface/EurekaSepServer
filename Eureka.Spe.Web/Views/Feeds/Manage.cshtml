@model Eureka.Spe.Web.Controllers.FeedsController.ManageFeedViewModel
@section Styles{

    <link href="~/js/bootstraptable/bootstrap-table.min.css" rel="stylesheet" />
    <link href="~/Views/Feeds/Manage.css" rel="stylesheet" />
}@{
    ViewBag.ActiveMenu = PageNames.Feeds; //The menu item will be active for this page.
    ViewBag.Title = "Noticias";
 }
<div class="row">
    <div class="col-lg-12 col-small-center">
        <a class="btn btn-lg bg-grey" href="@Url.Action("Index")"><i class="material-icons">keyboard_backspace</i> @Model.Feed.Title</a>
    </div>
</div>
<div class="row">
    <div class="col-lg-12 col-small-center ">
        <div class="card">
            <div class="body">
                <h4>@Model.Feed.Title</h4>
                <p><img class="img-responsive" style="width: 30px; height: 30px;display:inline-flex" src="@Model.Feed.PublisherImg" /> <strong>@Model.Feed.PublisherName</strong></p>
                <a href="@Url.Action("CreateOrEdit",new{id=Model.Feed.Id})" class="btn btn-primary">Editar contenido</a>
            </div>
        </div>
    </div>
</div>
<div class="row clearfix">
    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12 col-small-center">
        <div class="card">
            <div class="body" style="min-height: 500px">
                <input type="hidden" value="@Model.Feed.Id" id="Id" />
                <div class="row">
                    <div class="col-lg-12">
                        <ul class="nav nav-tabs" role="tablist">
                            <li role="presentation" class="active"><a href="#notifications" data-url="/Notifications/GetNotificationsForEntity?entityName=feeds&id=" aria-controls="notifications" role="tab" data-toggle="tab">Notificaciones</a></li>
                            <li role="presentation"><a href="#tags" data-url="/Layout/GetAcademicUnitsForEntity?entityName=feeds&id=" aria-controls="tags" role="tab" data-toggle="tab">Unidades Acádemicas</a></li>
                            <li role="presentation"><a href="#tags" data-url="/Stats/GetStatsForEntity?entityName=feeds&entityId=" aria-controls="tags" role="tab" data-toggle="tab">Estadísticas</a></li>
                        </ul>
                        <div class="tab-content">
                            <div role="tabpanel" class="tab-pane active" id="sections">

                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
@section Scripts{
    <script src="~/lib/jquery-validation/dist/jquery.validate.min.js"></script>
    <script src="~/lib/jquery-validation/dist/additional-methods.min.js"></script>
    <script src="~/js/eModal/eModal.min.js"></script>
    <script src="~/js/helpers.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/Chart.js/2.7.2/Chart.bundle.min.js"></script>
    <script src="/lib/jquery-sparkline/dist/jquery.sparkline.js"></script>
    <script src="~/Views/Feeds/Manage.js"></script>
}