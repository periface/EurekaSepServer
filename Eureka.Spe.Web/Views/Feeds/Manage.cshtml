@model Eureka.Spe.Web.Controllers.FeedsController.ManageFeedViewModel
@section Styles{

    <link href="~/js/bootstraptable/bootstrap-table.min.css" rel="stylesheet" />
}@{
    ViewBag.ActiveMenu = PageNames.AcademicUnits; //The menu item will be active for this page.
}
<div class="row clearfix">
    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
        <div class="card">
            <div class="body" style="min-height: 500px">

                <input type="hidden" value="@Model.Feed.Id" id="Id" />
                <div class="row">
                    <div class="col-lg-12">
                        <ul class="nav nav-tabs" role="tablist">
                            <li role="presentation" class="active"><a href="#notifications" data-url="/Feed/Notifications/" aria-controls="notifications" role="tab" data-toggle="tab">Notificaciones</a></li>
                            <li role="presentation"><a href="#tags" data-url="/Feed/AcademicUnits/" aria-controls="tags" role="tab" data-toggle="tab">Unidades Acádemicas</a></li>
                        </ul>
                        <div class="tab-content">
                            <div role="tabpanel" class="tab-pane active" id="sections">

                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
@section Scripts{
    <script src="~/lib/jquery-validation/dist/jquery.validate.min.js"></script>
    <script src="~/lib/jquery-validation/dist/additional-methods.min.js"></script>
    <script src="~/js/eModal/eModal.min.js"></script>
    <script src="~/js/helpers.js"></script>
    <script>
        (function () {
            console.log($("#Id").val());
            startTabListener($("#sections"), $("#Id").val());
        })();
    </script>

}