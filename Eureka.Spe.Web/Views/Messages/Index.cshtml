@section Styles{

    <link href="~/js/bootstraptable/bootstrap-table.min.css" rel="stylesheet" />
}@{
    ViewBag.ActiveMenu = PageNames.Messages; //The menu item will be active for this page.
     ViewBag.Title = "Mensajes del Rector"; 
}
<div class="row clearfix">
    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12 col-small-center">

        <a class="btn btn-primary btn-lg waves-effect pull-right js-add-message">
            Crear nuevo mensaje
        </a>
        <br /><br /><br />
        <div class="card">
            <div class="body table-responsive">
                <table data-pagination="true" data-classes="table table-no-bordered table-condensed"
                       data-side-pagination="server" data-events="buttons" data-search="true" data-url="/api/services/app/dialymessage/getall" id="table"></table>

            </div>
        </div>
    </div>
</div>
@section Scripts{
    <script src="~/lib/jquery-validation/dist/jquery.validate.min.js"></script>
    <script src="~/lib/jquery-validation/dist/additional-methods.min.js"></script>
    <script src="~/js/eModal/eModal.min.js"></script>
    <script src="~/js/bootstraptable/bootstrap-table.min.js"></script>
    <script src="~/js/bootstraptable/bootstrap-table-es-MX.min.js"></script>
    <script src="~/Views/Messages/Index.js"></script>
}