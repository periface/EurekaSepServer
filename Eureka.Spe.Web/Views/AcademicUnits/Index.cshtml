@section Styles{

    <link href="~/js/bootstraptable/bootstrap-table.min.css" rel="stylesheet" />
}@{
    ViewBag.ActiveMenu = PageNames.AcademicUnits; //The menu item will be active for this page.
}
<div class="row clearfix">
    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
        <div class="card">
            <div class="header">
                <h2>
                    @L("AcademicUnits")
                </h2>
            </div>
            <div class="body table-responsive">
                <table data-pagination="true"
                       data-side-pagination="server" data-events="buttons" data-search="true" data-url="/api/services/app/academicunit/getall" id="table" class="table table-responsive table-condensed"></table>
                <a class="btn btn-primary btn-circle waves-effect waves-circle waves-float pull-right js-add-academicUnit">
                    <i class="material-icons">add</i>
                </a>
            </div>
        </div>
    </div>
</div>
@section Scripts{
    <script src="~/lib/jquery-validation/dist/jquery.validate.min.js"></script>
    <script src="~/lib/jquery-validation/dist/additional-methods.min.js"></script>
    <script src="~/js/eModal/eModal.min.js"></script>
    <script src="~/js/bootstraptable/bootstrap-table.min.js"></script>
    <script src="~/js/bootstraptable/bootstrap-table-es-MX.min.js"></script>
    <script src="~/Views/AcademicUnits/Index.js"></script>

}