
@section Styles{

    <link href="~/js/bootstraptable/bootstrap-table.min.css" rel="stylesheet" />
}@{
    ViewBag.ActiveMenu = PageNames.Courses; //The menu item will be active for this page.
     ViewBag.Title = "Categorias";
}
<div class="row">
    <div class="col-lg-12 col-small-center">
        <a class="btn btn-primary btn-lg waves-effect js-add-category pull-right">
            Crear nueva categoría
        </a>
        <a href="/Courses" class="btn btn-default btn-lg waves-effect pull-left">
           Ir a Cursos
        </a>
        <br /><br /><br />
    </div>
</div>
<div class="row clearfix">
    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12 col-small-center">
        
        <div class="card">
            <div class="body table-responsive">
                <table data-pagination="true" data-side-pagination="server" data-classes="table table-no-bordered table-condensed" data-events="buttons" data-search="true" data-url="/api/services/app/courseCategories/getall" id="table"></table>

            </div>
        </div>
    </div>
</div>
@section Scripts{
    <script src="~/lib/jquery-validation/dist/jquery.validate.min.js"></script>
    <script src="~/lib/jquery-validation/dist/additional-methods.min.js"></script>
    <script src="~/js/eModal/eModal.min.js"></script>
    <script src="~/js/bootstraptable/bootstrap-table.min.js"></script>
    <script src="~/js/bootstraptable/bootstrap-table-es-MX.min.js"></script>
    <script src="~/js/helpers.js"></script>
    <script src="~/Views/CourseCategories/Index.js"></script>
}
