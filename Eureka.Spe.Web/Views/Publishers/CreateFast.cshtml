@model Eureka.Spe.Publishers.Dto.PublisherDto

@{
    Layout = null;
}
<form id="AddEditPublisherForm" role="form" novalidate class="form-validation">
    <input type="hidden" name="Id" value="@Model.Id" />
    <input type="hidden" name="TenantId" value="@Model.TenantId" />
    <input type="hidden" name="Img" value="@Model.Img" />
    <div class="row clearfix">
        <div class="col-sm-12">
            <div class="form-group form-float">
                <div class="form-line">
                    <input id="name" type="text" name="Name" value="@Model.Name" required maxlength="32" class="validate form-control">
                    <label for="name" class="form-label">@L("Name")</label>
                </div>
            </div>
        </div>
    </div>
</form>
<script>
    (function() {
        var form = $("#AddEditPublisherForm");
        $.AdminBSB.input.activate(form);
    })();
</script>